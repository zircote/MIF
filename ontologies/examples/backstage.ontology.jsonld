      "@container": "@id"
    },
    "requires": {
      "@id": "mif:requiresField",
      "@container": "@list"
    },
    "relationships": {
      "@id": "mif:definesRelationship",
      "@container": "@id"
    },
    "from": {
      "@id": "rdfs:domain",
      "@container": "@set"
    },
    "to": {
      "@id": "rdfs:range",
      "@container": "@set"
    },
    "symmetric": {
