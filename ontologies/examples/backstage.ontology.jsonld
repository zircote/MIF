    },
    "patterns": {
      "@id": "mif:hasDiscoveryPattern",
      "@container": "@list"
    },
    "confidence_threshold": {
      "@id": "mif:confidenceThreshold",
      "@type": "xsd:decimal"
    },
    "content_pattern": {
      "@id": "mif:contentPattern",
      "@type": "xsd:string"
    },
    "file_pattern": {
      "@id": "mif:filePattern",
      "@type": "xsd:string"
    },
    "suggest_entity": {
      "@id": "mif:suggestsEntityType",
      "@type": "@id"
    },
    "suggest_namespace": {
      "@id": "mif:suggestsNamespace",
      "@type": "xsd:string"
    }
  },
  "@id": "mif:ontology/backstage",
  "@type": "mif:Ontology",
  "identifier": "backstage",
  "version": "1.0.0",
  "description": "Backstage.io software catalog ontology",
  "schema_url": "https://github.com/zircote/MIF/schema/ontology/v2",
  "namespaces": {
    "semantic": {
      "@id": "mif:namespace/semantic",
      "@type": "mif:Namespace",
      "name": "semantic",
      "children": {
        "components": {
          "@id": "mif:namespace/semantic/components",
          "@type": "mif:Namespace",
          "name": "components",
          "description": "Software components, services, libraries, websites",
          "type_hint": "semantic"
        },
        "apis": {
          "@id": "mif:namespace/semantic/apis",
          "@type": "mif:Namespace",
          "name": "apis",
          "description": "API definitions and interfaces",
          "type_hint": "semantic"
        },
        "resources": {
          "@id": "mif:namespace/semantic/resources",
          "@type": "mif:Namespace",
          "name": "resources",
          "description": "Infrastructure and external resources",
          "type_hint": "semantic"
        },
        "systems": {
          "@id": "mif:namespace/semantic/systems",
          "@type": "mif:Namespace",
          "name": "systems",
          "description": "Collections of components forming products",
          "type_hint": "semantic"
        },
        "domains": {
          "@id": "mif:namespace/semantic/domains",
          "@type": "mif:Namespace",
          "name": "domains",
          "description": "Business or product area groupings",
          "type_hint": "semantic"
        },
        "groups": {
          "@id": "mif:namespace/semantic/groups",
          "@type": "mif:Namespace",
          "name": "groups",
          "description": "Teams, business units, organizational groups",
          "type_hint": "semantic"
        },
        "users": {
          "@id": "mif:namespace/semantic/users",
          "@type": "mif:Namespace",
          "name": "users",
          "description": "Individual people and service accounts",
          "type_hint": "semantic"
        },
        "locations": {
          "@id": "mif:namespace/semantic/locations",
          "@type": "mif:Namespace",
          "name": "locations",
          "description": "Catalog file locations and references",
          "type_hint": "semantic"
        },
        "templates": {
          "@id": "mif:namespace/semantic/templates",
          "@type": "mif:Namespace",
          "name": "templates",
          "description": "Software templates for scaffolding",
          "type_hint": "semantic"
        }
      }
    },
    "episodic": {
      "@id": "mif:namespace/episodic",
      "@type": "mif:Namespace",
      "name": "episodic",
      "children": {
        "catalog-events": {
          "@id": "mif:namespace/episodic/catalog-events",
          "@type": "mif:Namespace",
          "name": "catalog-events",
          "description": "Catalog changes, entity updates",
          "type_hint": "episodic"
        },
        "deployments": {
          "@id": "mif:namespace/episodic/deployments",
          "@type": "mif:Namespace",
          "name": "deployments",
          "description": "Deployment and release events",
          "type_hint": "episodic"
        },
        "incidents": {
          "@id": "mif:namespace/episodic/incidents",
          "@type": "mif:Namespace",
          "name": "incidents",
          "description": "Service incidents and outages",
          "type_hint": "episodic"
        }
      }
    },
    "procedural": {
      "@id": "mif:namespace/procedural",
      "@type": "mif:Namespace",
      "name": "procedural",
      "children": {
        "scaffolding": {
          "@id": "mif:namespace/procedural/scaffolding",
          "@type": "mif:Namespace",
          "name": "scaffolding",
          "description": "Template scaffolding procedures",
          "type_hint": "procedural"
        },
        "onboarding": {
          "@id": "mif:namespace/procedural/onboarding",
          "@type": "mif:Namespace",
          "name": "onboarding",
          "description": "Service onboarding workflows",
          "type_hint": "procedural"
        },
        "docs-publishing": {
          "@id": "mif:namespace/procedural/docs-publishing",
          "@type": "mif:Namespace",
          "name": "docs-publishing",
          "description": "TechDocs publishing procedures",
          "type_hint": "procedural"
        }
      }
    }
  },
  "entity_types": [
    {
      "@id": "mif:entityType/component",
      "@type": "mif:EntityType",
      "name": "component",
      "base": "semantic",
      "description": "A software component (service, website, library, etc.)",
      "traits": [
        "mif:trait/lifecycle",
        "mif:trait/owned",
        "mif:trait/tagged",
        "mif:trait/documented"
      ],
      "schema": {
        "required": [
          "name",
          "type",
          "lifecycle",
          "owner"
        ],
        "properties": {
          "name": {
            "type": "string",
            "description": "Component name (kebab-case)",
            "pattern": "^[a-z0-9]+(-[a-z0-9]+)*$"
          },
          "type": {
            "type": "string",
            "description": "Component type",
            "enum": [
              "service",
              "website",
              "library",
              "documentation",
              "template",
              "other"
            ]
          },
          "lifecycle": {
            "type": "string",
            "description": "Development lifecycle stage",
            "enum": [
              "experimental",
              "development",
              "production",
              "deprecated"
            ]
          },
          "owner": {
            "type": "string",
            "description": "Owning group reference"
          },
          "system": {
            "type": "string",
            "description": "Parent system reference"
          },
          "subcomponent_of": {
            "type": "string",
            "description": "Parent component for nested components"
          },
          "provides_apis": {
            "type": "array",
            "description": "APIs provided by this component",
            "items": {
              "type": "string"
            }
          },
          "consumes_apis": {
            "type": "array",
            "description": "APIs consumed by this component",
            "items": {
              "type": "string"
            }
          },
          "depends_on": {
            "type": "array",
            "description": "Components and resources this depends on",
            "items": {
              "type": "string"
            }
          }
        }
      }
    },
    {
      "@id": "mif:entityType/api",
      "@type": "mif:EntityType",
      "name": "api",
      "base": "semantic",
      "description": "An API interface definition",
      "traits": [
        "mif:trait/lifecycle",
        "mif:trait/owned",
        "mif:trait/versioned"
      ],
      "schema": {
        "required": [
          "name",
          "type",
          "lifecycle",
          "owner",
          "definition"
        ],
        "properties": {
          "name": {
            "type": "string",
            "description": "API name"
          },
          "type": {
            "type": "string",
            "description": "API specification type",
            "enum": [
              "openapi",
              "asyncapi",
              "graphql",
              "grpc",
              "trpc"
            ]
          },
          "lifecycle": {
            "type": "string",
            "enum": [
              "experimental",
              "development",
              "production",
              "deprecated"
            ]
          },
          "owner": {
            "type": "string"
          },
          "system": {
            "type": "string"
          },
          "definition": {
            "type": "string",
            "description": "API definition (inline or $text reference)"
          }
        }
      }
    },
    {
      "@id": "mif:entityType/resource",
      "@type": "mif:EntityType",
      "name": "resource",
      "base": "semantic",
      "description": "Infrastructure or external resource",
      "traits": [
        "mif:trait/lifecycle",
        "mif:trait/owned"
      ],
      "schema": {
        "required": [
          "name",
          "type",
          "owner"
        ],
        "properties": {
          "name": {
            "type": "string"
          },
          "type": {
            "type": "string",
            "description": "Resource type",
            "enum": [
              "database",
              "s3-bucket",
              "cdn",
              "cluster",
              "queue",
              "cache",
              "storage",
              "artifact-registry",
              "secret-store",
              "vault",
              "other"
            ]
          },
          "owner": {
            "type": "string"
          },
          "system": {
            "type": "string"
          },
          "depends_on": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
      }
    },
    {
      "@id": "mif:entityType/system",
      "@type": "mif:EntityType",
      "name": "system",
      "base": "semantic",
      "description": "A collection of components forming a product/capability",
      "traits": [
        "mif:trait/owned",
        "mif:trait/tagged"
      ],
      "schema": {
        "required": [
          "name",
          "owner"
        ],
        "properties": {
          "name": {
            "type": "string"
          },
          "owner": {
            "type": "string"
          },
          "domain": {
            "type": "string",
            "description": "Parent domain reference"
          }
        }
      }
    },
    {
      "@id": "mif:entityType/domain",
      "@type": "mif:EntityType",
      "name": "domain",
      "base": "semantic",
      "description": "A business or product area grouping systems",
      "traits": [
        "mif:trait/owned"
      ],
      "schema": {
        "required": [
          "name",
          "owner"
        ],
        "properties": {
          "name": {
            "type": "string"
          },
          "owner": {
            "type": "string"
          }
        }
      }
    },
    {
      "@id": "mif:entityType/group",
      "@type": "mif:EntityType",
      "name": "group",
      "base": "semantic",
      "description": "A team, business unit, or organizational group",
      "traits": [
        "mif:trait/contactable"
      ],
      "schema": {
        "required": [
          "name",
          "type"
        ],
        "properties": {
          "name": {
            "type": "string"
          },
          "type": {
            "type": "string",
            "description": "Group type",
            "enum": [
              "team",
              "business-unit",
              "product-area",
              "organization",
              "sub-department"
            ]
          },
          "parent": {
            "type": "string",
            "description": "Parent group reference"
          },
          "children": {
            "type": "array",
            "description": "Child group references",
            "items": {
              "type": "string"
            }
          },
          "members": {
            "type": "array",
            "description": "User references",
            "items": {
              "type": "string"
            }
          },
          "profile": {
            "type": "object",
            "properties": {
              "display_name": {
                "type": "string"
              },
              "email": {
                "type": "string",
                "format": "email"
              },
              "picture": {
                "type": "string",
                "format": "uri"
              },
              "slack": {
                "type": "string"
              },
              "pagerduty": {
                "type": "string"
              }
            }
          }
        }
      }
    },
    {
      "@id": "mif:entityType/user",
      "@type": "mif:EntityType",
      "name": "user",
      "base": "semantic",
      "description": "An individual person or service account",
      "traits": [
        "mif:trait/contactable"
      ],
      "schema": {
        "required": [
          "name"
        ],
        "properties": {
          "name": {
            "type": "string",
            "description": "Username (typically email prefix)"
          },
          "member_of": {
            "type": "array",
            "description": "Group memberships",
            "items": {
              "type": "string"
            }
          },
          "profile": {
            "type": "object",
            "properties": {
              "display_name": {
                "type": "string"
              },
              "email": {
                "type": "string",
                "format": "email"
              },
              "picture": {
                "type": "string",
                "format": "uri"
              },
              "github": {
                "type": "string"
              },
              "slack": {
                "type": "string"
              },
              "pagerduty": {
                "type": "string"
              }
            }
          }
        }
      }
    },
    {
      "@id": "mif:entityType/location",
      "@type": "mif:EntityType",
      "name": "location",
      "base": "semantic",
      "description": "A reference to catalog file locations",
      "schema": {
        "required": [
          "name",
          "type",
          "target"
        ],
        "properties": {
          "name": {
            "type": "string"
          },
          "type": {
            "type": "string",
            "description": "Location type",
            "enum": [
              "url",
              "file",
              "github",
              "gitlab",
              "bitbucket",
              "azure-devops"
            ]
          },
          "target": {
            "type": "string",
            "description": "Target URL or path"
          },
          "presence": {
            "type": "string",
            "description": "Whether location is required",
            "enum": [
              "required",
              "optional"
            ]
          }
        }
      }
    },
    {
      "@id": "mif:entityType/template",
      "@type": "mif:EntityType",
      "name": "template",
      "base": "procedural",
      "description": "A software template for scaffolding new projects",
      "traits": [
        "mif:trait/versioned",
        "mif:trait/owned",
        "mif:trait/tagged"
      ],
      "schema": {
        "required": [
          "name",
          "type",
          "owner"
        ],
        "properties": {
          "name": {
            "type": "string"
          },
          "type": {
            "type": "string",
            "enum": [
              "service",
              "website",
              "documentation",
              "library",
              "other"
            ]
          },
          "owner": {
            "type": "string"
          },
          "parameters": {
            "type": "array",
            "description": "Template input parameters",
            "items": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string"
                },
                "title": {
                  "type": "string"
                },
                "description": {
                  "type": "string"
                },
                "type": {
                  "type": "string"
                },
                "required": {
                  "type": "boolean"
                },
                "default": {
                  "type": "string"
                }
              }
            }
          },
          "steps": {
            "type": "array",
            "description": "Scaffolding steps",
            "items": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "string"
                },
                "name": {
                  "type": "string"
                },
                "action": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    {
      "@id": "mif:entityType/annotation",
      "@type": "mif:EntityType",
      "name": "annotation",
      "base": "semantic",
      "description": "Well-known catalog annotation",
      "schema": {
        "required": [
          "key",
          "description"
        ],
        "properties": {
          "key": {
            "type": "string",
            "description": "Annotation key (domain/name format)"
          },
          "description": {
            "type": "string"
          },
          "value_type": {
            "type": "string",
            "enum": [
              "string",
              "url",
              "json"
            ]
          },
          "example": {
            "type": "string"
          }
        }
      }
    },
    {
      "@id": "mif:entityType/catalog-refresh",
      "@type": "mif:EntityType",
      "name": "catalog-refresh",
      "base": "episodic",
      "description": "Catalog refresh/sync event",
      "schema": {
        "required": [
          "location_id",
          "timestamp",
          "status"
        ],
        "properties": {
          "location_id": {
            "type": "string"
          },
          "timestamp": {
            "type": "string",
            "format": "date-time"
          },
          "status": {
            "type": "string",
            "enum": [
              "success",
              "partial",
              "failed"
            ]
          },
          "entities_processed": {
            "type": "integer"
          },
          "errors": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
      }
    },
    {
      "@id": "mif:entityType/techdocs-build",
      "@type": "mif:EntityType",
      "name": "techdocs-build",
      "base": "episodic",
      "description": "TechDocs build event",
      "schema": {
        "required": [
          "entity_ref",
          "timestamp",
          "status"
        ],
        "properties": {
          "entity_ref": {
            "type": "string",
            "description": "Entity reference (kind:namespace/name)"
          },
          "timestamp": {
            "type": "string",
            "format": "date-time"
          },
          "status": {
            "type": "string",
            "enum": [
              "success",
              "failed"
            ]
          },
          "build_duration_seconds": {
            "type": "number"
          },
          "output_url": {
            "type": "string",
            "format": "uri"
          }
        }
      }
    },
    {
      "@id": "mif:entityType/scaffolder-task",
      "@type": "mif:EntityType",
      "name": "scaffolder-task",
      "base": "episodic",
      "description": "Template scaffolding task execution",
      "traits": [
        "mif:trait/lifecycle"
      ],
      "schema": {
        "required": [
          "template_ref",
          "created_by",
          "status"
        ],
        "properties": {
          "template_ref": {
            "type": "string"
          },
          "created_by": {
            "type": "string"
          },
          "status": {
            "type": "string",
            "enum": [
              "pending",
              "running",
              "completed",
              "failed",
              "cancelled"
            ]
          },
          "parameters": {
            "type": "object"
          },
          "output": {
            "type": "object",
            "properties": {
              "repo_url": {
                "type": "string"
              },
              "catalog_entity": {
                "type": "string"
              }
            }
          }
        }
      }
    }
  ],
  "traits": {
    "documented": {
      "@id": "mif:trait/documented",
      "@type": "mif:Trait",
      "name": "documented",
      "description": "Adds TechDocs and documentation fields",
      "fields": {
        "techdocs_ref": {
          "type": "string",
          "description": "TechDocs entity reference"
        },
        "docs_url": {
          "type": "string",
          "format": "uri",
          "description": "External documentation URL"
        }
      }
    }
  },
  "relationships": {
    "owned_by": {
      "@id": "mif:relationship/owned_by",
      "@type": "mif:Relationship",
      "name": "owned_by",
      "description": "Entity is owned by a group",
      "from": [
        "mif:entityType/component",
        "mif:entityType/api",
        "mif:entityType/resource",
        "mif:entityType/system",
        "mif:entityType/domain",
        "mif:entityType/template"
      ],
      "to": [
        "mif:entityType/group"
      ],
      "symmetric": false
    },
    "part_of": {
      "@id": "mif:relationship/part_of",
      "@type": "mif:Relationship",
      "name": "part_of",
      "description": "Entity is part of a system/domain",
      "from": [
        "mif:entityType/component",
        "mif:entityType/api",
        "mif:entityType/resource"
      ],
      "to": [
        "mif:entityType/system"
      ],
      "symmetric": false
    },
    "has_part": {
      "@id": "mif:relationship/has_part",
      "@type": "mif:Relationship",
      "name": "has_part",
      "description": "System has component parts",
      "from": [
